{
  "expo": {
    "name": "BookMate",
    "slug": "bookmate-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "owner": "siamoon",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "assets/fonts/**",
      "assets/icon.png",
      "assets/images/**",
      "assets/lottie/**"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.siamoon.bookmate",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to scan receipts.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload receipts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#121212"
      },
      "package": "com.siamoon.bookmate",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow BookMate to access your camera to scan receipts."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow BookMate to access your photos to upload receipts."
        }
      ],
      "expo-asset",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ]
  }
}
